{
  "buildCommand": "npm run build:staging",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/batch_download.html",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html; charset=utf-8"
        }
      ]
    },
    {
      "source": "/download_config.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json; charset=utf-8"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/batch_download.html",
      "dest": "/batch_download.html"
    },
    {
      "src": "/download_config.json",
      "dest": "/download_config.json"
    }
  ]
}
